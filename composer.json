{
    "name": "sokil/user-bundle",
    "description": "User bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Sokil",
            "email": "dmytro.sokil@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "sokil/command-bus-bundle": "^0.4.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7.38 <6.0",
        "satooshi/php-coveralls": "1.0.*",
        "symfony/form": "^2.8||^3.0",
        "symfony/framework-bundle": "^2.8 || ^3.0",
        "doctrine/doctrine-bundle": "^1.3",
        "symfony/serializer": "^2.8|^3.0",
        "symfony/validator": "^2.8||^3.0",
        "sensio/framework-extra-bundle": "^2.8||~3.0,>=3.0.2"
    },
    "autoload": {
        "psr-4": {
            "Sokil\\UserBundle\\": ["src/", "tests/"]
        }
    },
    "suggest": {
        "sokil/frontend-bundle": "Required to enable SPA",
        "sokil/deploy-bundle": "Deploy symfony2 apps"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
