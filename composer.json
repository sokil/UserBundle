{
    "name": "sokil/user-bundle",
    "description": "User bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Sokil",
            "email": "dmytro.sokil@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "sokil/command-bus-bundle": "^0.4"
    },
    "require-dev": {
        "sokil/frontend-bundle": "^0.12",
        "phpunit/phpunit": "5.4.*",
        "satooshi/php-coveralls": "1.0.*",
        "symfony/form": "^2.7 || ^3.0",
        "symfony/framework-bundle": "^2.7 || ^3.0",
        "doctrine/doctrine-bundle": "^1.3",
        "symfony/serializer": ">=2.8.0|~3.0",
        "symfony/validator": "^2.7 || ^3.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2"
    },
    "autoload": {
        "psr-4": {
            "Sokil\\UserBundle\\": ["src/", "tests/"]
        }
    },
    "suggest": {
        "sokil/frontend-bundle": "Required to enable SPA",
        "sokil/deploy-bundle": "Deploy symfony2 apps"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
